<?xml version="1.0" encoding="utf-8"?>
<robot name="rex" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <material name="white">
        <color rgba="1 1 1 1" />
    </material>
    <material name="black">
        <color rgba="0.1 0.1 0.1 1" />
    </material>
    <material name="foot_color">
        <color rgba="0 0.75 1 1" />
    </material>

    <xacro:macro name="box_inertia" params="m x y z">
        <inertial>
            <mass value="${m}" />
            <inertia ixx="${(m/12) * (y*y + z*z)}" ixy="0.0" ixz="0.0"
                     iyy="${(m/12) * (x*x + z*z)}" iyz="0.0"
                     izz="${(m/12) * (x*x + y*y)}" />
        </inertial>
    </xacro:macro>
    <!-- Params -->
    <!-- Macros -->
    <!-- Robot Body -->
<link name="base_footprint"/>

    <link name="chassis_link"/>
    <joint name="base_joint" type="fixed">
        <parent link="base_footprint"/>
        <child link="chassis_link"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <link name="base_link">
        <visual>
            <geometry>
                <mesh filename="file://$(find spotmicro_description)/urdf/stl/mainbody.stl" scale="0.001 0.001 0.001" />
            </geometry>
            <material name="black" />
            <origin rpy="0 0 0" xyz="-0.045 -0.060 -0.015" />
        </visual>
        <collision>
            <geometry>
                <box size="0.14 0.11 0.07" />
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0" />
        </collision>
        <xacro:box_inertia m="1.0" x="0.14" y="0.11" z="0.07" />
    </link>

    <joint name="base_flip_joint" type="fixed">
        <parent link="chassis_link"/>
        <child link="base_link"/>
        <origin xyz="0 0 0" rpy="0 0 3.14159"/>
